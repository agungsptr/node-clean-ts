{
  "name": "node-clean-ts",
  "version": "1.0.0",
  "description": "A TypeScript version of node-clean",
  "main": "./drivers/webservice",
  "repository": "https://github.com/agungsptr/node-clean-ts.git",
  "scripts": {
    "start": "node ./dist/drivers/webservice",
    "dev": "nodemon ./drivers/webservice/index.ts",
    "tsc": "tsc",
    "grpc": "nodemon ./dist/drivers/grpc/index.ts",
    "grpc_client": "NODE_ENV=test node ./dist/drivers/grpc/client-examples",
    "seed": "NODE_ENV=test ts-node ./db/seeds",
    "lint": "eslint .",
    "test": "NODE_ENV=test mocha -r ts-node/register --exit --exclude ./node_modules/**/*.spec.ts ./**/*.spec.ts",
    "coverage_test": "NODE_ENV=test nyc --reporter=text npm run test",
    "load_test": "NODE_ENV=test artillery run --dotenv ./.env --output ./test/output/loadTest.json ./test/loadTest.yml",
    "load_test-result": "artillery report --output ./test/output/report.html ./test/output/loadTest.json"
  },
  "keywords": [
    "nodejs",
    "clean-architecture",
    "mongo",
    "express",
    "typescript"
  ],
  "author": "agungsptr",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.9",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "helmet": "^7.0.0",
    "joi": "^17.10.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.9",
    "@types/chai-as-promised": "^7.1.5",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/eslint": "^8.44.2",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.11.16",
    "@types/nodemon": "^1.19.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.61.0",
    "artillery": "^2.0.0-37",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.51.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
